<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ShuttleSkyfallerBase">
    <defName>Carryall_standard_Leaving</defName>
    <label>standard carryall (leaving)</label>	
	<thingClass>SRTS.SRTSLeaving</thingClass>
    <graphicData>
      <texPath>Carryall/CarryallBasic</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutFlying</shaderType>
      <drawSize>(4.2,4.2)</drawSize>
	  <color>(255,255,255)</color>
    </graphicData>
    <skyfaller>
      <shadow>Things/Skyfaller/SkyfallerShadowRectangle</shadow>
      <shadowSize>(4,2)</shadowSize>
      <motesPerCell>1</motesPerCell>	
      <reversed>true</reversed>
      <anticipationSound>Shuttle_Leaving</anticipationSound>
      <anticipationSoundTicks>-10</anticipationSoundTicks>
      <ticksToImpactRange>-40~-15</ticksToImpactRange>
      <moteSpawnTime>0.05</moteSpawnTime>
      <angleCurve>
        <points>
          <li>(0,90)</li>
          <li>(1,70)</li>
        </points>
      </angleCurve>
      <rotationCurve>
        <points>
          <li>(90,90)</li>
          <li>(90.15,100)</li>
          <li>(90.5,85)</li>
        </points>
      </rotationCurve>
      <zPositionCurve>
        <points>
          <li>(0,0)</li>
          <li>(0.08,2)</li>
        </points>
      </zPositionCurve>
      <speedCurve>
        <points>
          <li>(0,0.2)</li>
          <li>(0.4,0.7)</li>
        </points>
      </speedCurve>
    </skyfaller>
  </ThingDef>

<ThingDef ParentName="ShuttleSkyfallerBase">
    <defName>Carryall_standard_Incoming</defName>
    <label>standard carryall (incoming)</label>
	<thingClass>SRTS.SRTSIncoming</thingClass>
    <graphicData>
      <texPath>Carryall/CarryallBasic</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutFlying</shaderType>
      <drawSize>(4.2,4.2)</drawSize>
	  <color>(255,255,255)</color>
    </graphicData>
    <skyfaller>
      <shadow>Things/Skyfaller/SkyfallerShadowRectangle</shadow>
      <shadowSize>(4,2)</shadowSize>
      <motesPerCell>1</motesPerCell>
      <anticipationSound>Shuttle_Landing</anticipationSound>
      <anticipationSoundTicks>250</anticipationSoundTicks>
      <ticksToImpactRange>200~250</ticksToImpactRange>
      <angleCurve>
        <points>
          <li>(0,-60)</li>
          <li>(1,-90)</li>
        </points>
      </angleCurve>
      <rotationCurve>
        <points>
          <li>(-90,-120)</li>
          <li>(-90.5,-95)</li>
          <li>(-90.9,-95)</li>
          <li>(-90.95,-90)</li>
        </points>
      </rotationCurve>
      <zPositionCurve>
        <points>
          <li>(0.95,2.5)</li>
          <li>(1,0)</li>
        </points>
      </zPositionCurve>
      <speedCurve>
        <points>
          <li>(0.6,0.6)</li>
          <li>(0.95,0.1)</li>
        </points>
      </speedCurve>
      <!--<explosionRadius>4</explosionRadius>
      <explosionDamage>Bomb</explosionDamage>-->
    </skyfaller>
  </ThingDef>
  
  <ThingDef>
    <defName>Carryall_standard_Active</defName>
    <label>standard carryall</label>
