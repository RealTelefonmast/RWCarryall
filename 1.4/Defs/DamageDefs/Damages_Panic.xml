<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <DamageDef ParentName="Bomb">
    <defName>Lands_Carryall</defName>
    <label>crush</label>
	<deathMessage>{0} has has been crushed by a carryall to death.</deathMessage>
    <externalViolence>true</externalViolence>
	<armorCategory>Blunt</armorCategory>
    <defaultDamage>50</defaultDamage>
	<defaultStoppingPower>2</defaultStoppingPower>
	<defaultArmorPenetration>0.8</defaultArmorPenetration>
    <explosionSnowMeltAmount>10</explosionSnowMeltAmount>
    <explosionCellFleck>Smoke_Carryall</explosionCellFleck>
	<explosionColorCenter>(0.9, 0.9, 0.9, 0.15)</explosionColorCenter>
    <explosionColorEdge>(0.9, 0.9, 0.9, 0.15)</explosionColorEdge>
    <explosionInteriorFleck>Smoke_Interior_Carryall</explosionInteriorFleck>
    <soundExplosion>GunTail_Heavy</soundExplosion>
  </DamageDef>

  <FleckDef ParentName="FleckBase_Thrown">
    <defName>Smoke_Carryall</defName>
    <graphicData>
      <texPath>Misc/CarryallGas</texPath>
      <renderInstanced>true</renderInstanced>
    </graphicData>
    <altitudeLayer>MoteOverhead</altitudeLayer>
    <fadeInTime>0.5</fadeInTime>
    <solidTime>3</solidTime>
    <fadeOutTime>8</fadeOutTime>
    <growthRate>0.004</growthRate>
  </FleckDef>

  <FleckDef ParentName="FleckBase_Thrown">
    <defName>Smoke_Interior_Carryall</defName>
    <graphicData>
      <texPath>Misc/CarryallGas</texPath>
      <renderInstanced>true</renderInstanced>
    </graphicData>
    <altitudeLayer>MoteOverhead</altitudeLayer>
    <fadeInTime>1</fadeInTime>
    <solidTime>2</solidTime>
    <fadeOutTime>6</fadeOutTime>
    <growthRate>0.002</growthRate>
  </FleckDef>

</Defs>
